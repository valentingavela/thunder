  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.{js,scss}": [
      "npm run prettier",
      "git add"
    ],
    "web/**/*.{tsx,ts}": [
      "node utils-dev/compileStaged.js",
